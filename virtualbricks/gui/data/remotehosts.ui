<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.20"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="window1">
    <property name="can_focus">False</property>
    <child>
      <object class="GtkTreeView" id="remotehosts_treeview">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <signal name="button-press-event" handler="on_remotehosts_treeview_button_press_event" swapped="no"/>
        <signal name="button-release-event" handler="on_remotehosts_treeview_button_release_event" swapped="no"/>
        <child>
          <object class="GtkTreeViewColumn" id="status_treeviewcolumn">
            <property name="title" translatable="yes">Status</property>
            <child>
              <object class="GtkCellRendererPixbuf" id="status_cellrenderer"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkTreeViewColumn" id="address_treeviewcolumn">
            <property name="title" translatable="yes">Address</property>
            <property name="clickable">True</property>
            <property name="sort_column_id">1</property>
            <child>
              <object class="GtkCellRendererText" id="address_cellrenderer"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkTreeViewColumn" id="numbrick_treeviewcolumn">
            <property name="title" translatable="yes">Brick</property>
            <property name="reorderable">True</property>
            <property name="sort_column_id">2</property>
            <child>
              <object class="GtkCellRendererText" id="numbrick_cellrenderer"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkTreeViewColumn" id="ac_treeviewcolumn">
            <property name="title" translatable="yes">Autoconnect</property>
            <property name="reorderable">True</property>
            <property name="sort_column_id">3</property>
            <child>
              <object class="GtkCellRendererText" id="ac_cellrenderer"/>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
