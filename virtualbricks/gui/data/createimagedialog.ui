    <data>
      <row>
        <col id="0" translatable="yes">Auto</col>
      </row>
      <row>
        <col id="0" translatable="yes">raw</col>
      </row>
      <row>
        <col id="0" translatable="yes">qcow</col>
      </row>
      <row>
        <col id="0" translatable="yes">qcow2</col>
      </row>
      <row>
