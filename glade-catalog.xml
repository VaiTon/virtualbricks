<glade-catalog name="virtualbricks.gui.widgets"
    version="0.1"
    supports="gtkbuilder"
    domain="virtualbricks"
    library="gladepython"
    requires="gtk+">

    <init-function>glade_python_init</init-function>

    <glade-widget-classes>
        <glade-widget-class
            title="TreeView"
            name="TreeView"
            generic-name="treeview" />
        <glade-widget-class
            title="ComboBox"
            name="ComboBox"
            generic-name="combobox" />
        <glade-widget-class
            title="List"
            name="List"
            generic-name="liststore">
            <properties>
            </properties>
        </glade-widget-class>
        <glade-widget-class
            title="CellRendererFormattable"
            name="CellRendererFormattable"
            generic-name="cellrenderer" />
        <glade-widget-class
            title="CellRendererBrickIcon"
            name="CellRendererBrickIcon"
            generic-name="cellrenderer" />
        <glade-widget-class
            title="TreeViewColumn"
            name="TreeViewColumn"
            generic-name="treeviewcolumn" />
    </glade-widget-classes>

    <glade-widget-group title="Virtualbricks" name="virtualbricks">
        <glade-widget-class-ref name="TreeView" />
        <glade-widget-class-ref name="ComboBox" />
        <glade-widget-class-ref name="List" />
        <glade-widget-class-ref name="CellRendererFormattable" />
        <glade-widget-class-ref name="CellRendererBrickIcon" />
        <glade-widget-class-ref name="TreeViewColumn" />
    </glade-widget-group>
</glade-catalog>
